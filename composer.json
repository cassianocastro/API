{
    "name": "students-api/students-api",
    "description": "A simple example of Rest API",
    "type": "project",
    "license": "MIT",
	"readme": "./README.md",
	"prefer-stable": true,
    "minimum-stability": "stable",
	"keywords": [
		"api",
		"students",
		"REST"
	],
    "authors": [
        {
            "name": "Cassiano Castro",
            "email": "development.castro@gmail.com",
            "role": "Developer"
        }
    ],
    "autoload-dev": {
        "psr-4": {
			""                     : "src/",
            "Models\\"             : "src/model/",
            "Models\\Helpers\\"    : "src/model/helpers/",
            "Models\\Creational\\" : "src/model/creational/",
            "Models\\Entities\\"   : "src/model/entities/",
            "Models\\Tables\\"     : "src/model/tables/",
            "Routes\\"             : "src/routes/",
            "Controllers\\"        : "src/controller/",
            "Views\\"              : "src/view/"
        }
    },
	"config": {
		"preferred-install": "dist",
		"optimize-autoloader": false,
		"sort-packages": true
	},
    "require": {
		"php": ">=8.1.2",
		"ext-mbstring": ">=8.1.2"
	}
}