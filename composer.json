{
    "name": "students-api/students-api",
    "description": "A simple example of Rest API",
    "type": "project",
    "license": "MIT",
	"readme": "./README.md",
	"prefer-stable": true,
    "minimum-stability": "stable",
	"keywords": [
		"api",
		"students",
		"REST"
	],
    "authors": [
        {
            "name": "Cassiano Castro",
            "email": "development.castro@gmail.com",
            "role": "Lead"
        }
    ],
    "autoload": {
        "psr-4": {
			"Api\\": "src/api/"
        }
    },
	"autoload-dev": {
        "psr-4": {
			"Tests\\": "src/tests/"
        }
    },
	"config": {
		"preferred-install": "dist",
		"optimize-autoloader": false,
		"sort-packages": true
	},
    "require": {
        "php": ">=8.1.2",
        "ext-mbstring": ">=8.1.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    }
}